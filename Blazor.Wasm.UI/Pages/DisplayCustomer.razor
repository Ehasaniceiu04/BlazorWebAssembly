

<tr>
    <th scope="row">@customer.Id</th>
    <td>@customer.FirstName</td>
    <td>@customer.LastName</td>
    <td>@customer.Email</td>
    <td>@customer.GenderId</td>
    <td class="float-end">
        <button type="button" class="btn btn-primary" @onclick="@(()=>OnHandleEdit(@customer))">Edit</button>
        <button type="button" @onclick=@(()=>OnHandleDelete(@customer)) class="btn btn-danger">Delete</button></td>
</tr>





